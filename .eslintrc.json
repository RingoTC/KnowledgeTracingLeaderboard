{
  "extends": "next/core-web-vitals",
  "rules": {
    "@typescript-eslint/no-unused-vars": "error",
    "@typescript-eslint/no-explicit-any": "error",
    "react/no-unescaped-entities": "off",
    "@next/next/no-assign-module-variable": "off"
  },
  "ignorePatterns": [
    ".next/**/*",
    "node_modules/**/*",
    "out/**/*",
    "**/.next/**/*",
    "**/node_modules/**/*",
    "**/out/**/*"
  ]
} 